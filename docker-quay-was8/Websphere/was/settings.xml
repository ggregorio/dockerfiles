<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <pluginGroups>
        <pluginGroup>org.sonarsource.scanner.maven</pluginGroup>
    </pluginGroups>
    <proxies>
        <proxy>
            <id>optional</id>
            <active>true</active>
            <protocol>http</protocol>
            <host>proxy.ar.bsch</host>
            <port>8080</port>
            <nonProxyHosts>*.ar.bsch</nonProxyHosts>
        </proxy>
    </proxies>
    <servers>
        <server>
            <id>maven-public</id>
            <username>anonymous</username>
        </server>
		<server>
            <id>nexus</id>
            <username>deploy-nexus</username>
			<password>deploy</password>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <id>maven-public</id>
            <mirrorOf>*</mirrorOf>
            <name>Human Readable Name for this Mirror.</name>
            <url>http://nexus.iaas.ar.bsch/repository/maven-public</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>isban</id>
            <activation>
                <jdk>1.8</jdk>
            </activation>
        </profile>
        <profile>
            <id>sonar</id>
            <activation>
                <activeByDefault>
          true
        </activeByDefault>
            </activation>
            <properties>
                <sonar.host.url>
                    http://sonarprod.ar.bsch:9000/
                </sonar.host.url>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>isban</activeProfile>
    </activeProfiles>
</settings>
